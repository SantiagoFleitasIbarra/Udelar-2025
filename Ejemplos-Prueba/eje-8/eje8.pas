program eje8;

begin
    
    writeln('El resultado de i es: ', 5 + sqr(3) - 4 + trunc(3.6 - 2.1));
    writeln('El resultado de ii es: ', 3 * sqrt(8 MOD 6 * 10 DIV 5));
    writeln('El resultado de iii es: ', 6 + 9 * 8 DIV 2 * round(1.362) - 2 * 3);
